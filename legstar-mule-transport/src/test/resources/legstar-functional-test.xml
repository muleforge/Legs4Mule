<?xml version="1.0" encoding="UTF-8"?>
<mule xmlns="http://www.mulesource.org/schema/mule/core/2.2"
    xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xmlns:spring="http://www.springframework.org/schema/beans"
    xmlns:legstar="http://www.mulesource.org/schema/mule/legstar/2.2"
    xmlns:test="http://www.mulesource.org/schema/mule/test/2.2"
    xsi:schemaLocation="
       http://www.mulesource.org/schema/mule/test/2.2 http://www.mulesource.org/schema/mule/test/2.2/mule-test.xsd
       http://www.springframework.org/schema/beans http://www.springframework.org/schema/beans/spring-beans-2.5.xsd
       http://www.mulesource.org/schema/mule/core/2.2 http://www.mulesource.org/schema/mule/core/2.2/mule.xsd
       http://www.mulesource.org/schema/mule/legstar/2.2 http://www.legsem.com/legstar/xml/mule-legstar-1.0.xsd">

    <legstar:endpoint name="clientEndpoint"
        address="legstar:http://localhost:60199" synchronous="true" />
    <legstar:host-to-java-transformer
        name="LsfileaeHostToJava"
        class="org.mule.transport.legstar.transformer.LsfileaeHostToJavaTransformer" />
    <legstar:java-to-host-transformer
        name="LsfileaeJavaToHost"
        class="org.mule.transport.legstar.transformer.LsfileaeJavaToHostTransformer" />

    <model name="main">
        <custom-service name="testComponent"
            class="org.mule.tck.testmodels.mule.TestSedaService">
            <inbound>
                <inbound-endpoint ref="clientEndpoint"
                    transformer-refs="LsfileaeHostToJava"
                    responseTransformer-refs="LsfileaeJavaToHost" />
            </inbound>
            <test:component />
        </custom-service>
    </model>

</mule>
